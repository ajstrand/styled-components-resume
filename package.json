{
  "name": "styled-components-resume-generator",
  "version": "0.0.1",
  "description": "generator that creates a resume with styled components, html and css grid",
  "author": "ajstrand",
  "license": "MIT",
  "repository": "ajstrand/styled-components-resume-generator",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "tsc --outDir build/lib --declarationDir build/types --declaration true && rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "npm run prebuild:storybook && npm run lint:css && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint:css": "stylelint src/**/*.tsx"
  },
  "dependencies": {
    "@types/lodash": "^4.14.161",
    "babel-plugin-polished": "^1.1.0",
    "core-js": "^3.6.5",
    "polished": "^3.6.6",
    "prop-types": "^15.7.2",
    "regenerator-runtime": "^0.13.7"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.13.1",
    "react-dom": "^15.0.0 || ^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-jest": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.21",
    "@svgr/rollup": "^5.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.8.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "node-sass": "^4.14.1",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.2",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.13.1",
    "require-context.macro": "^1.2.2",
    "rollup": "^2.26.11",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.0",
    "sass-loader": "^8.0.2",
    "storybook-addon-jsx": "^7.3.4",
    "style-loader": "^1.2.1",
    "styled-components": "^5.2.0",
    "styled-theming": "^2.2.0",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
