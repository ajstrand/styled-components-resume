{
  "name": "styled-components-resume-generator",
  "version": "1.0.0",
  "description": "generator that creates a resume with styled components, html and css grid",
  "author": "ajstrand",
  "license": "MIT",
  "repository": "ajstrand/styled-components-resume-generator",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "babel-plugin-polished": "^1.1.0",
    "core-js": "^3.6.1",
    "polished": "^3.4.2",
    "regenerator-runtime": "^0.13.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-storyshots": "^5.2.8",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.2.8",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/node": "^13.1.4",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/storybook__addon-storyshots": "^5.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "^6.0.3",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gh-pages": "^2.1.1",
    "jest-styled-components": "^6.3.4",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.12.0",
    "require-context.macro": "^1.2.2",
    "rollup": "^1.28.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-url": "^3.0.1",
    "storybook-addon-jsx": "^7.1.13",
    "styled-components": "^4.4.1",
    "styled-theming": "^2.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {
      "^.+\\.jsx?$": ".jest/transform.ts"
    },
    "setupFiles": [
      ".jest/register-context.js"
    ]
  }
}
