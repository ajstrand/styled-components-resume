{
  "name": "styled-resume",
  "version": "0.0.1",
  "description": "generator that creates a resume with styled components, html and css grid",
  "author": "ajstrand",
  "license": "MIT",
  "repository": "ajstrand/styled-resume-generator",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "tsc --outDir build/lib --declarationDir build/types --declaration true && rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "npm run prebuild:storybook && npm run lint:css && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint:css": "stylelint src/**/*.tsx"
  },
  "dependencies": {
    "@types/lodash": "^4.14.165",
    "babel-plugin-polished": "^1.1.0",
    "core-js": "^3.8.1",
    "polished": "^4.0.5",
    "prop-types": "^15.7.2",
    "regenerator-runtime": "^0.13.7"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^17.0.1",
    "react-dom": "^15.0.0 || ^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-jest": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-storyshots": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.11",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "eslint": "^7.16.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "node-sass": "^4.14.1",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.2",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.14.0",
    "require-context.macro": "^1.2.2",
    "rollup": "^2.35.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^2.6.1",
    "sass-loader": "^8.0.2",
    "storybook-addon-jsx": "^7.3.4",
    "style-loader": "^1.3.0",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
