{
  "name": "styled-components-resume-generator",
  "version": "1.0.0",
  "description": "generator that creates a resume with styled components, html and css grid",
  "author": "ajstrand",
  "license": "MIT",
  "repository": "ajstrand/styled-components-resume-generator",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "npm run lint:css && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint:css": "stylelint src/**/*.tsx"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "babel-plugin-polished": "^1.1.0",
    "core-js": "^3.6.4",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "regenerator-runtime": "^0.13.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "@rollup/plugin-url": "^4.0.2",
    "@storybook/addon-a11y": "^5.3.12",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-storyshots": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.12",
    "@svgr/rollup": "^5.1.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.1",
    "jest-styled-components": "^7.0.0",
    "node-sass": "^4.13.1",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.12.0",
    "require-context.macro": "^1.2.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^2.0.6",
    "rollup-plugin-scss": "^2.1.0",
    "sass-loader": "^8.0.2",
    "storybook-addon-jsx": "^7.1.14",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "styled-theming": "^2.2.0",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": ".jest/transform.ts"
    },
    "setupFiles": [
      ".jest/register-context.js"
    ]
  }
}
